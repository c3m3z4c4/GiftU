{
    "version": 2,
    "name": "Gift√ú",
    "builds": [
        { "use": "@vercel/static-build", "src": "package.json" }
    ],
    "routes": [{
            "src": "(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },
        {
            "src": "(.*).png",
            "dest": "/$1.png"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}